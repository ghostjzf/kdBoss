<view class='container'>
  <picker 
    class='section-input' 
    name="type" 
    mode = 'multiSelector'
    bindchange="bindPickerChange"
    bindcolumnchange="bindMultiPickerColumnChange"
    value="{{multiIndex}}" range="{{multiArray}}">
    <view class="picker">{{multiArray[0][multiIndex[0]]}}-{{multiArray[1][multiIndex[1]]}}</view>
  </picker>
</view>